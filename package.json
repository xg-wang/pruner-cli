{
  "name": "pruner-cli",
  "version": "0.3.0",
  "description": "Prune node_modules files and save disk space",
  "repository": {
    "type": "git",
    "url": "https://github.com/xg-wang/pruner-cli"
  },
  "keywords": [
    "cli",
    "prune",
    "node_modules"
  ],
  "main": "lib/cli.js",
  "bin": {
    "prune": "lib/cli.js",
    "pruner-cli": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "prepare": "yarn build",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "changelog": "lerna-changelog"
  },
  "author": "Thomas Wang<wangxgwxg@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^8.1.0",
    "yargs": "^13.2.4"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "devDependencies": {
    "@types/chai": "^4.0.5",
    "@types/fs-extra": "^8.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.1",
    "@types/yargs": "^13.0.0",
    "chai": "^4.1.2",
    "husky": "^3.0.0",
    "lerna-changelog": "^0.8.2",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-node": "^8.3.0",
    "typescript": "~3.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
