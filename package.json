{
  "name": "pruner-cli",
  "version": "0.3.0",
  "description": "Prune node_modules files and save disk space",
  "repository": {
    "type": "git",
    "url": "https://github.com/xg-wang/pruner-cli"
  },
  "keywords": [
    "cli",
    "prune",
    "node_modules"
  ],
  "main": "lib/cli.js",
  "bin": {
    "prune": "lib/cli.js",
    "pruner-cli": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "prepare": "yarn build",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "changelog": "lerna-changelog"
  },
  "author": "Thomas Wang<wangxgwxg@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^9.1.0",
    "yargs": "^16.2.0"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/fs-extra": "^9.0.6",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@types/yargs": "^15.0.12",
    "chai": "^4.1.2",
    "husky": "^4.3.8",
    "lerna-changelog": "^1.0.1",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-node": "^9.1.1",
    "typescript": "~4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
